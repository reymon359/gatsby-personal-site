{
  "name": "gatsby-personal-site",
  "private": true,
  "description": "A Gatsby Typescript project for my personal site",
  "version": "1.0.0",
  "author": "Ramon Morcillo @reymon359",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --write",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc",
    "type-check:watch": "tsc -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run type-check && npm run build"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.11.1",
    "classnames": "2.2.6",
    "dotenv": "8.2.0",
    "gatsby": "2.24.77",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-canonical-urls": "2.3.13",
    "gatsby-plugin-google-analytics": "2.3.18",
    "gatsby-plugin-manifest": "2.4.34",
    "gatsby-plugin-offline": "3.2.31",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.6.42",
    "gatsby-plugin-sitemap": "2.4.16",
    "gatsby-plugin-styled-components": "3.3.14",
    "gatsby-plugin-typescript": "2.4.21",
    "gatsby-remark-autolink-headers": "2.3.15",
    "gatsby-remark-copy-linked-files": "2.3.19",
    "gatsby-remark-embed-video": "^3.0.10",
    "gatsby-remark-images": "3.3.33",
    "gatsby-remark-prismjs": "3.5.16",
    "gatsby-remark-responsive-iframe": "2.4.17",
    "gatsby-remark-smartypants": "2.3.13",
    "gatsby-remark-video": "^1.2.5",
    "gatsby-source-filesystem": "2.3.34",
    "gatsby-transformer-json": "2.4.14",
    "gatsby-transformer-remark": "2.8.39",
    "gatsby-transformer-sharp": "2.5.17",
    "prism-themes": "1.4.1",
    "prismjs": "1.22.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-tippy": "1.4.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "none yet",
  "repository": {
    "type": "git",
    "url": "https://github.com/reymon359/gatsby-personal-site"
  },
  "bugs": {
    "url": "https://github.com/reymon359/gatsby-personal-site/issues"
  }
}
