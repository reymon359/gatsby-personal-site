{
  "name": "gatsby-personal-site",
  "private": true,
  "description": "A Gatsby Typescript project for my personal site",
  "version": "1.0.0",
  "author": "Ramon Morcillo @reymon359",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc",
    "type-check:watch": "tsc -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run type-check && npm run build"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "classnames": "2.3.1",
    "dotenv": "8.2.0",
    "gatsby": "2.32.12",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-canonical-urls": "2.10.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-react-svg": "3.0.1",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-plugin-typescript": "2.12.1",
    "gatsby-remark-autolink-headers": "2.11.0",
    "gatsby-remark-copy-linked-files": "2.10.0",
    "gatsby-remark-embed-video": "3.1.1",
    "gatsby-remark-images": "3.11.1",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-remark-responsive-iframe": "2.11.0",
    "gatsby-remark-smartypants": "2.10.0",
    "gatsby-remark-video": "1.2.5",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-json": "2.11.0",
    "gatsby-transformer-remark": "2.16.1",
    "gatsby-transformer-sharp": "2.12.1",
    "prism-themes": "1.7.0",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-tippy": "1.4.0",
    "styled-components": "5.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/classnames": "2.3.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.44",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-helmet": "6.1.1",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "7.25.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.1.0",
    "husky": "4.3.8",
    "typescript": "4.2.4"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "none yet",
  "repository": {
    "type": "git",
    "url": "https://github.com/reymon359/gatsby-personal-site"
  },
  "bugs": {
    "url": "https://github.com/reymon359/gatsby-personal-site/issues"
  }
}
