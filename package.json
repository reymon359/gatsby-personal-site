{
  "name": "gatsby-personal-site",
  "private": true,
  "description": "A Gatsby Typescript project for my personal site",
  "version": "0.1.0",
  "author": "Ramon Morcillo @reymon359",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --write",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc",
    "type-check:watch": "tsc -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run type-check && npm run build"
    }
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "^1.1.2",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.20",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-canonical-urls": "^2.3.9",
    "gatsby-plugin-google-analytics": "^2.3.10",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-plugin-sitemap": "^2.4.10",
    "gatsby-plugin-styled-components": "^3.3.9",
    "gatsby-plugin-typescript": "^2.4.13",
    "gatsby-remark-autolink-headers": "^2.3.10",
    "gatsby-remark-copy-linked-files": "^2.3.10",
    "gatsby-remark-highlight-code": "^1.4.5",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-remark-prismjs": "^3.5.9",
    "gatsby-remark-responsive-iframe": "^2.4.10",
    "gatsby-remark-smartypants": "^2.3.9",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-json": "^2.4.10",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.10",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-tippy": "^1.4.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.14",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "none yet",
  "repository": {
    "type": "git",
    "url": "https://github.com/reymon359/gatsby-personal-site"
  },
  "bugs": {
    "url": "https://github.com/reymon359/gatsby-personal-site/issues"
  }
}
